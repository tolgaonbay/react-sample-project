{
    "ecmaFeatures": {
        "jsx": true
    },
    "env": {
        "browser": true,
        "node": true,
        "jquery": true
    },
    "rules": {
        "quotes": 0,
        "no-trailing-spaces": 0,
        "eol-last": 0,
        "no-unused-vars": "error",
        "no-underscore-dangle": 0,
        "no-alert": "error",
        "no-lone-blocks": 0,
        "no-undef": "error"
    },
    "globals": {
        "jQuery": true,
        "$": true
    }
}
